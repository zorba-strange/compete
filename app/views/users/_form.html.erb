<%= form_for @user  do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :profile_url %>
  <%= f.text_field :profile_url %>

  <%= f.label :bio %>
  <%= f.text_field :bio %>

  <%= f.submit "Submit" %>
<% end %>
